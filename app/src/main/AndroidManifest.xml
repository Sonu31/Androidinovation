<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidInovation"
        tools:targetApi="31">

        <activity
            android:name=".jetpack.allActivity.ToolbarActivity"
            android:exported="false"
            android:label="@string/title_activity_toolbar"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.ScrollActivity"
            android:exported="false"
            android:label="@string/title_activity_scroll"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.AnimatioActivity"
            android:exported="false"
            android:label="@string/title_activity_animatio"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.BNavigationActivity"
            android:exported="false"
            android:label="@string/title_activity_bnavigation"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.DialogActivity"
            android:exported="false"
            android:label="@string/title_activity_dialog"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.ListActivity"
            android:exported="false"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.ButtonActivity"
            android:exported="false"
            android:label="@string/title_activity_button"
            android:theme="@style/Theme.AndoridJerpack" />
        <activity
            android:name=".jetpack.allActivity.TextActivity"
            android:exported="false"
            android:label="@string/title_activity_text"
            android:theme="@style/Theme.AndoridJerpack" />



        <activity
            android:name=".jetpack.JetpackHomeActivity"
            android:exported="false"
            android:label="@string/title_activity_jetpack_main"
            android:theme="@style/Theme.AndroidInovation" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>